{
  "name": "syncedup-universal-platform",
  "version": "2.0.0",
  "description": "Universal Business Management Platform - Industry-Agnostic SRM",
  "main": "index.js",
  "scripts": {
    "dev": "vercel dev",
    "build": "echo 'Nuclear transformation build complete'",
    "start": "vercel --prod",
    "deploy": "vercel --prod",
    "deploy:insurance": "vercel --prod --env INDUSTRY=insurance",
    "deploy:healthcare": "vercel --prod --env INDUSTRY=healthcare",
    "deploy:realestate": "vercel --prod --env INDUSTRY=realestate",
    "deploy:finance": "vercel --prod --env INDUSTRY=finance",
    "deploy:manufacturing": "vercel --prod --env INDUSTRY=manufacturing"
  },
  "keywords": [
    "universal-platform",
    "industry-agnostic",
    "business-management",
    "insurance",
    "healthcare",
    "real-estate",
    "finance",
    "manufacturing",
    "nuclear-transformation"
  ],
  "author": "SyncedUp Solutions",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vercel/node": "^3.0.0"
  },
  "engines": {
    "node": "18.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/syncedupadmin/genericportal.git"
  },
  "vercel": {
    "framework": null,
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "outputDirectory": "public"
  }
}